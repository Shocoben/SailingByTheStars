void MyFragmentShader1(float2 uv: TEXCOORD0, out float4 color: COLOR, uniform sampler2D texture)
{
	color = tex2D(texture, uv);
}

void MyVertexShader1(float4 position : POSITION, out float4 oPosition : POSITION, float2 uv : TEXCOORD0, out float2 oUV : TEXCOORD0, uniform float4x4 worldViewMatrix)
{

	oPosition = mul(worldViewMatrix, position);
	oUV = uv;
}